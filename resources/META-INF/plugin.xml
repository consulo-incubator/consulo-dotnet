<idea-plugin>
	<id>org.mustbe.consulo.dotnet.core</id>
	<name>.NET Core</name>

	<application-components>
		<component>
			<implementation-class>org.mustbe.consulo.dotnet.dll.vfs.DotNetArchiveFileSystem</implementation-class>
		</component>
	</application-components>

	<extensions defaultExtensionNs="com.intellij">
		<fileTypeFactory implementation="org.mustbe.consulo.dotnet.dll.DotNetDllFileTypeFactory" />

		<configurationType implementation="org.mustbe.consulo.dotnet.run.DotNetConfigurationType"/>

		<compiler implementation="org.mustbe.consulo.dotnet.compiler.DotNetCompiler"/>

		<projectService serviceInterface="org.mustbe.consulo.dotnet.compiler.DotNetCompilerConfiguration"
						serviceImplementation="org.mustbe.consulo.dotnet.compiler.DotNetCompilerConfiguration"/>
	</extensions>
</idea-plugin>